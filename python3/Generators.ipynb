   "source": [
    "def next_square():\n",
    "    i = 1\n",
    "    while True:\n",
    "        yield i * i\n",
    "        i += 1\n",
    "\n",
    "\n",
    "for n in next_square():\n",
    "    if n > 25:\n",
    "        break\n",
    "    print(n)"
   ]
